<%= form_for @permit, url: wizard_path do |f| %>
  <h2>Enter Address</h2>
  <div class="field">
    <%= f.label :owner_address %><br />
    <%= f.text_field :owner_address %>
  </div>
  <div class="actions">
    <%= f.submit "Continue" %>
    or <%= link_to "skip this step", next_wizard_path %>
  </div>
<% end %>